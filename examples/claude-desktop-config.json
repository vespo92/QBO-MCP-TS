{
  "mcpServers": {
    "quickbooks-ts": {
      "command": "node",
      "args": [
        "/Users/vinnieespo/Projects/VCPU/VinOrg2025/MCPServers/QBOMCP-TS/dist/index.js",
        "--transport",
        "stdio"
      ],
      "env": {
        "QBO_CLIENT_ID": "your_client_id_here",
